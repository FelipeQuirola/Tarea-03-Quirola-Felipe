import math.h
def sumatorio(inicio, fin, f):
    resultado = 0
    for i in range(inicio, fin + 1):
        resultado += f(i)
    return resultado

def funcion(x):
    return 1 / (x ** 2)

resp = sumatorio (1, 10, funcion)
